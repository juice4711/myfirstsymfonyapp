{% extends '::base.html.twig' %}

{% block body -%}
   <div id="clear">
		{% for flashMessage in app.session.flashbag.get('notice') %}
		<div class="flash-notice">
			{{ flashMessage }}
		</div>
		{% endfor %}
	</div>
	<div id="leftSide">
		<fieldset><legend class="buttonlegend">
			<span class="buttonspan" onclick="document.location.href = '{{ path('admin_supportuser_edit', { 'id': entity.id }) }}'">Support User</span></legend>
		{{ entity.avatar|raw }}
		<table class='rightTable'>
		  <tbody>
		   <tr class="orderlist">
		        <td class='orderlisttd'>User Name</td>
			<td class='orderlisttd'>{{ entity.userName }}</td>
		   </tr>
		   <tr class="orderlist  highlight">
		        <td class='orderlisttd'>Given Name</td>
		<td>{{ entity.givenName }}</td>
		</tr>
		   <tr class="orderlist">
		        <td class='orderlisttd'>User Email</td>
			<td class='orderlisttd'>{{ entity.userEmail }}</td>
		   </tr>
		   <tr class="orderlist  highlight">
		        <td class='orderlisttd'>User Email Alt</td>
		<td>{{ entity.userEmailAlt }}</td>
		</tr>
		   <tr class="orderlist">
		        <td class='orderlisttd'>User Contact</td>
		<td>{{ entity.userContact }}</td>
		</tr>
		   <tr class="orderlist  highlight">
		        <td class='orderlisttd'>User Contact Alt</td>
		<td>{{ entity.userContactAlt }}</td>
		</tr>
		   <tr class="orderlist">
		        <td class='orderlisttd'>User Contact Internal</td>
		<td>{{ entity.userContactInternal }}</td>
		</tr>
		   <tr class="orderlist highlight">
		        <td class='orderlisttd'>Cerberus Id</td>
		<td>{{ entity.cerberusID }}</td>
		</tr>
		</tbody>
	    </table>
	  </fieldset>
	</div>

	{{ render(controller('ExclusiveBooksDemoAppBundle:Admin:mainmenu')) }}

{% endblock %}

