rest_smsaudit_show:
    path:     /{id}/show
    defaults: { _controller: "ExclusiveBooksDemoAppBundle:Rest:show" }
    requirements:
        _method: GET
rest_smsaudit_showall:
    path:     /showall
    defaults: { _controller: "ExclusiveBooksDemoAppBundle:Rest:showAll" }
    requirements:
        _method: GET


